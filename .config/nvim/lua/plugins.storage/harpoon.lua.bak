return {
    "ThePrimeagen/harpoon",
    enabled = false,
    branch = "harpoon2",
    -- lazy = false,
    dependencies = {
        "nvim-lua/plenary.nvim",
        "letieu/harpoon-lualine",
    },
    opts = {
        menu = {
            -- width = vim.api.nvim_win_get_width(0) - 4,
            -- width = 0.2,
        },
        settings = {
            save_on_toggle = true,
        }
    },
    keys = function()
        local keys = {
            {
                "<C-A-S-Find>q",
                function()
                    local toggle_opts = { ui_width_ratio = 0.3, border = "bold" }
                    local harpoon = require("harpoon")
                    harpoon.ui:toggle_quick_menu(harpoon:list(), toggle_opts)
                end,
                desc = "Harpoon Menu",
            },
            {
                "<C-space>",
                function()
                    require("harpoon"):list():add()
                end,
                desc = "Hapoon File",
            },
            -- {
            --     "C-A-S-F12>e",
            --     function()
            --         require("harpoon"):list():select(1)
            --     end,
            --     desc = "Open Harpoon 1",
            -- }
        }

        for i, key in ipairs({"w", "e", "r", "t", "y"}) do
            table.insert(keys, {
                "<C-A-S-Find>" .. key,
                function()
                    require("harpoon"):list():select(i)
                end,
                desc = "Open Harpoon " .. i,
            })
        end
        return keys
    end,
}
